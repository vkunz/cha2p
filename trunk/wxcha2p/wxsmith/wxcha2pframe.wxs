<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="wxcha2pFrame">
		<size>400,490</size>
		<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="m_text_output" member="yes">
			<pos>0,0</pos>
			<size>400,320</size>
			<style>wxTE_MULTILINE|wxTE_READONLY</style>
		</object>
		<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="m_text_input" member="yes">
			<pos>0,336</pos>
			<size>352,112</size>
		</object>
		<object class="wxButton" name="ID_BUTTON1" variable="Button_Send" member="yes">
			<label>Send</label>
			<pos>352,336</pos>
			<size>48,112</size>
			<handler function="OnButton_SendClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxMenuBar" variable="MenuBar1" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuConnect" variable="MenuItem3" member="yes">
					<label>Connect</label>
					<accel>Alt-C</accel>
				</object>
				<object class="wxMenuItem" name="idMenuDisconnect" variable="MenuItem4" member="yes">
					<label>Disconnect</label>
					<accel>Alt-D</accel>
				</object>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>1</fields>
			<widths>-1</widths>
			<styles>wxSB_NORMAL</styles>
		</object>
	</object>
</wxsmith>
